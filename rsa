#!/usr/bin/env python3
import prime

import sys
import time
primes = prime.arr





if __name__ == '__main__':
    if len(sys.argv) != 2:
        print("Usage: python rsa <file>")
        sys.exit(1)

    file_path = sys.argv[1]
    num = 1
    factor1 = 1
    factor2 = 1
    with open(file_path, "r") as file:
       
        for line in file:
            num = int(line)
        for x in primes:
            if num % x == 0:
                factor1 = x
                break
        factor2 = num // factor1
    print(f"{num}={factor1}*{factor2}")
    